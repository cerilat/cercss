:root{
    --accent-light-1: hsl(240,75%,75%);
    --accent-light-2: hsl(240,65%,65%);
    --accent-light-3: hsl(240,55%,55%);
    --accent-light-4: hsl(240,45%,45%);
    --accent-dark-2: hsl(240,35%,25%);
    --accent-dark-1: hsl(240,25%,15%);
    --dark-1: hsl(240,15%,10%);
    --dark-2: hsl(240,15%,12.5%);
    --dark-3: hsl(240,15%,15%);
    --dark-4: hsl(240,15%,20%);
    --light-1: hsl(240, 100%, 95%);
    --light-2: hsl(240,25%,80%);
    --light-3: hsl(240,15%,65%);
    --light-4: hsl(240,15%,40%);
    --btn-light-transparent: hsla(240, 100%, 80%, .1);
    --btn-light-transparent-hover: hsla(240,75%,80%,.15);
    --btn-dark-transparent: hsla(240,25%,5%,.1);
    --btn-dark-transparent-hover: hsla(240,25%,5%,.15);
    
    --ic-brand-primary: var(--accent-light-2);
    --ic-brand-primary-darkened-5: #144432;
    --ic-brand-primary-darkened-10: #13402F;
    --ic-brand-primary-darkened-15: #123D2D;
    --ic-brand-primary-lightened-5: #20503E;
    --ic-brand-primary-lightened-10: #2C5948;
    --ic-brand-primary-lightened-15: #386252;
    --ic-brand-button--primary-bgd-darkened-5: #144432;
    --ic-brand-button--primary-bgd-darkened-15: #123D2D;
    --ic-brand-button--secondary-bgd-darkened-5: #26333D;
    --ic-brand-button--secondary-bgd-darkened-15: #222E37;
    --ic-brand-font-color-dark-lightened-15: var(--light-4);
    --ic-brand-font-color-dark-lightened-28: var(--light-3);
    --ic-brand-font-color-dark: var(--light-2);
    --ic-link-color: var(--accent-light-1);
    --ic-link-color-darkened-10: var(--accent-light-2);
    --ic-link-color-lightened-10: var(--accent-light-2);
    --ic-brand-button--primary-bgd: var(--ic-brand-primary);
    --ic-brand-button--primary-text: var(--light-1);
    --ic-brand-button--secondary-bgd: var(--light-3);
    --ic-brand-button--secondary-text: var(--light-1);
    --ic-brand-global-nav-bgd: var(--dark-3);
    --ic-brand-global-nav-ic-icon-svg-fill: var(--light-2);
    --ic-brand-global-nav-ic-icon-svg-fill--active: var(--light-1);
    --ic-brand-global-nav-menu-item__text-color: var(--light-2);
    --ic-brand-global-nav-menu-item__text-color--active: var(--light-1);
    --ic-brand-global-nav-avatar-border: var(--accent-light-2);
    --ic-brand-global-nav-menu-item__badge-bgd: #FFFF00;
    --ic-brand-global-nav-menu-item__badge-bgd--active: var(--ic-brand-primary);
    --ic-brand-global-nav-menu-item__badge-text: #000000;
    --ic-brand-global-nav-menu-item__badge-text--active: var(--light-1);
    --ic-brand-global-nav-logo-bgd: var(--light-1);
    --ic-brand-header-image: url('https://instructure-uploads-pdx.s3.us-west-2.amazonaws.com/account_152790000000000001/attachments/60/vertical_logo-300x211.png');
    --ic-brand-mobile-global-nav-logo: url('https://instructure-uploads-pdx.s3.us-west-2.amazonaws.com/account_152790000000000001/attachments/1953/vertical_logo-300x211.png');
    --ic-brand-watermark: ;
    --ic-brand-watermark-opacity: 1.0;
    --ic-brand-favicon: url('https://du11hjcvx0uqb.cloudfront.net/dist/images/favicon-e10d657a73.ico');
    --ic-brand-apple-touch-icon: url('https://du11hjcvx0uqb.cloudfront.net/dist/images/apple-touch-icon-585e5d997d.png');
    --ic-brand-msapplication-tile-color: var(--ic-brand-primary);
    --ic-brand-msapplication-tile-square: url('https://du11hjcvx0uqb.cloudfront.net/dist/images/windows-tile-eda8889e7b.png');
    --ic-brand-msapplication-tile-wide: url('https://du11hjcvx0uqb.cloudfront.net/dist/images/windows-tile-wide-44d3cc1060.png');
    --ic-brand-right-sidebar-logo: ;
    --ic-brand-Login-body-bgd-color: #334451;
    --ic-brand-Login-body-bgd-image: ;
    --ic-brand-Login-body-bgd-shadow-color: #273540;
    --ic-brand-Login-logo: url('https://du11hjcvx0uqb.cloudfront.net/dist/images/login/canvas-logo-5617606953.svg');
    --ic-brand-Login-Content-bgd-color: none;
    --ic-brand-Login-Content-border-color: none;
    --ic-brand-Login-Content-inner-bgd: none;
    --ic-brand-Login-Content-inner-border: none;
    --ic-brand-Login-Content-inner-body-bgd: none;
    --ic-brand-Login-Content-inner-body-border: none;
    --ic-brand-Login-Content-label-text-color: #ffffff;
    --ic-brand-Login-Content-password-text-color: #ffffff;
    --ic-brand-Login-footer-link-color: #ffffff;
    --ic-brand-Login-footer-link-color-hover: #ffffff;
    --ic-brand-Login-instructure-logo: #ffffff;
}

html{
    scrollbar-color: var(--light-4) var(--dark-2);
}

body{
    background-color: var(--dark-1)
}
.ViewerControls{
    background-color: var(--dark-2);
    color: var(--ic-brand-font-color-dark);
}
.ViewerControls--current-page {
    background-color: var(--dark-4);
    color: var(--ic-brand-font-color-dark);
    border: 1px solid var(--light-4)
}
.AnnotationControlButton {
    background-color: transparent;
    color: var(--ic-brand-font-color-dark);
}
.AnnotationControlButton:hover:enabled {
    background-color: var(--btn-light-transparent-hover);
}
.AnnotationControlButton:focus .AnnotationControlButton-outer {
    border-color: var(--light-4);
}
.AnnotationControlButton-outer {
    border-color: transparent;
}
.AnnotationControlButton:hover:enabled .AnnotationControlButton-outer {
    border-color: transparent;
}
.AnnotationControlButton-inner svg path {
    fill: var(--ic-brand-font-color-dark);
}
div span[aria-hidden="true"]{
    color: var(--ic-brand-font-color-dark) !important;
}
polygon[fill="#F5F5F5"]{
    fill: var(--ic-brand-font-color-dark);
}

.Pages{
    background-color: var(--dark-1)
}
.Page-container[style="width: 974px; border-top: 1px dashed rgb(168, 168, 168);"]{
    border-top: 1px solid var(--light-4) !important
}
